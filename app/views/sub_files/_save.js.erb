$("ul.errors").html("")
<% if @sub_file.errors.any? %>
  <% @sub_file.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>

  $("#modals").modal("hide");
    location = "<%= subject_path(id: cookies[:current_subject], parent: @sub_file.parent_id) %>";
<% end %>